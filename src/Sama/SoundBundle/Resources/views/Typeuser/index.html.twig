{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Typeuser list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Idtypeuser</th>
                <th>Nom</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('typeuser_show', { 'id': entity.idtypeuser }) }}">{{ entity.idtypeuser }}</a></td>
                <td>{{ entity.nom }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('typeuser_show', { 'id': entity.idtypeuser }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('typeuser_edit', { 'id': entity.idtypeuser }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('typeuser_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
